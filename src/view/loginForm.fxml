<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="225.0" prefWidth="315.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.loginFormController">
   <children>
      <Label fx:id="loginLabel" layoutX="25.0" layoutY="25.0" text="Login">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label fx:id="loginUserNameLabel" layoutX="40.0" layoutY="70.0" text="Username" />
      <TextField fx:id="userNameTF" layoutX="140.0" layoutY="66.0" />
      <Label fx:id="loginPasswordLabel" layoutX="40.0" layoutY="110.0" text="Password" />
      <TextField fx:id="userPassTF" layoutX="140.0" layoutY="106.0" />
      <HBox alignment="BOTTOM_RIGHT" layoutX="25.0" layoutY="155.0" prefHeight="25.0" prefWidth="265.0" spacing="15.0">
         <children>
            <Button fx:id="loginSubmitButton" mnemonicParsing="false" onMouseClicked="#loginSubmit" text="Submit" />
         </children>
      </HBox>
      <Label fx:id="loginLocationLabel" layoutX="25.0" layoutY="200.0" text="Location:" />
   </children>
</AnchorPane>
